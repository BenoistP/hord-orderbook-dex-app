{
 "rules": {
    "no-unused-vars": "warn",
    "no-console": "warn"
 },
 "env": {
    "browser": true,
    "node": true
 },
 "plugins": [
    "react"
  ],
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true,
        "modules": true,
        "experimentalObjectRestSpread": true
      }
  }
}